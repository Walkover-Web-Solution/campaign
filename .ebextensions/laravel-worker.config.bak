Prerequisites 

sudo apt install php-mongodb composer


# Master Branch
- cd /home/ubuntu/phpapp
- git pull
# - ln -f -s .env.prod .env
- rm composer.lock // no need to run
- COMPOSER_MEMORY_LIMIT=-1 composer install --optimize-autoloader --no-dev
- sudo chown -R ubuntu:ubuntu *
- sudo chmod -R 755 storage
